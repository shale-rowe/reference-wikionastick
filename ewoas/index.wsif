	var re = /([\w\.\-]+)\(([^"\(\)]{4,})\)/g;
	for (var i = pages.length-1; i > 0; --i) {
		var pagetxt = pages[i];
		var res = pagetxt.toString().match(re);
		if (res != null) {
			resx += '<tr><td>' + res[0] +"</td><td>-</td><td>" + res[1] + '</td></tr>';
