// Micro JSON
function serialize(v){
 var a = [];
 for (var e in v){
  a.push(' ' + e + ': "' + v[e] + '"');
 }
 return "MY={\n" +a.join(",\n")+ "\n}";
}

	if(index>0){ // Page exists, so write variables
		try{
			pages[index]=serialize(MY);
			//alert('GameSaveVars: saved:'+pages[index])
		}catch(e){
			alert("FATAL: could not save " + e);
		}
	}else{
		alert("FATAL: page '"+VARPAGE+"' has not been created!")
	}
	woas._save_to_file(true);
	GAME.LoadVars();
}

GAME.Reset=function(){
	var VARPAGE = 'game::variables';
	var index = woas.page_index(VARPAGE);
	if(index>0){ // Page exists, so write variables
