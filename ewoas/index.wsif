{| class="mycustomtable"
|-
| =cell 1 || =cell 2 || =cell 3
|-
| cell 1 || cell 2 || cell 3
|-
| cell 1 || cell 2 || cell 3
|-
|}
</td><td valign="top">
{{{
{| class="mycustomtable"
|-
| =cell 1 || =cell 2 || =cell 3
|-
| cell 1 || cell 2 || cell 3
|-
| cell 1 || cell 2 || cell 3
|-
|}
}}}
</td></tr></table>

