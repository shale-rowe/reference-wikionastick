<!-- Macro's either are defined globally, or in the page. Do not transclude them as they run in closure --> \\
<<<%sum_from_to(N_FROM,N_TO, WHAT):

  var elements = [",,/n=N_TO/,," , "\u2211" , "^^/n=N_FROM/^^"];

  // Now add the function. We escape any division, as it can get italicized:
  elements[1] += '   /' + WHAT.replace(/\\//g,"~/") + '/';

  RESULT=0; // without 'var', it becomes a global variable, reusable in this page.

  var s="";
  for(var i=N_FROM ; i<N_TO ; i++){
    s = WHAT.replace(/n/g,i);
    try { RESULT += parseFloat(eval(s)) } catch(e) { RESULT="ERROR"; }
  };

  // Now add the result:
  elements[1] += ' = /' + RESULT + '/';

  // stack things one above the other:
  macro.text = '<span style="display:inline-block; vertical-align:middle">' 
    + elements.join("<br>").replace(/N_FROM/g, N_FROM).replace(/N_TO/g, N_TO)
    + "</span>";

  // And all output should be parsed as wiki text, (macro.reprocess = false; by default)
  macro.reprocess = 1;
>>>
