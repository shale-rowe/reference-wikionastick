<<<
wiki();
var lines= "%01".replace(/<br>/g, "\n");
if(lines != "%"+"01"){ // prevents an error when viewing this page, when there are no transcluded 
say("*Transclusion* started");
// catch possible Multiline String parameters and the page 'crashes'.
  lines=lines.split("\n");
  function TwoDec(n){return Math.round(n*100)/100};
  var ztable='<table border="0" cellspacing="0" cellpadding="0">';
  var ztd='<td style="border-bottom: 2px solid #6699CC;border-left: 1px solid #6699CC;background-color: #BEC8D1;text-align: left;text-indent: 5px;font-family: Verdana;font-weight: bold;font-size: 11px;color: #404040;">';
  var table='<table border="0" cellspacing="1" cellpadding="0"><tr>' +ztd+'label</td>'+ztd+'progress</td>'+ztd+'total hours</td>'+ztd+'hours to go</td>'+ztd+'%done</td></tr>';
  var totalhours=0;
  var totalprogress=0;
  for(var i=0;i<lines.length;i++){
    var item=lines[i].split(";")
    var label=item[0];
    if(!label)continue;
    item=item[1].split("/");
    var hours=parseFloat(item[1]);
    var progress=parseFloat(item[0]);
    if(progress>hours) hours=progress;
    totalhours+=hours;
    totalprogress+=progress;
    table+='<tr bgcolor=Gainsboro><td>'+label+'</td><td>';
    var green = Math.round(progress/hours*100);
    var red = 100 - green;

    table += ztable + '<tr height="12">'
     + (green>0? '<td width="'+green+'" bgcolor="green"></td>':'')
     + (red>0? '<td width="'+red+'" bgcolor="red"></td>':'')
     + '</tr></table>'
     + '<td style="text-align: right;">'+hours+'</td><td style="text-align: right;">'+TwoDec(hours-progress)+'</td><td style="text-align: right;">'+TwoDec(green)+'%</td></tr>';
  }
  if(lines.length>1)
    table+='<tr><td></td><td></td><td style="text-align: right;font-weight:bold;font-family: Verdana;font-size: 11px">'+TwoDec(totalhours)
      +'</td><td style="text-align: right;font-weight:bold;font-family: Verdana;font-size: 11px">'+TwoDec(totalprogress)
      +'</td><td style="text-align: right;font-weight:bold;font-family: Verdana;font-size: 11px">'+TwoDec(totalprogress/totalhours*100)+'%</td></tr>';

  table+='</table>';
  print( table );  // woas.parser.parse(

}else{
  print("Edit me to see the source. LINES=%01")
}>>>
