/* NewPageTemplate plugin */

// You must change this function to create something fancy

woas._new_page_template = function(pg) {
	if(pg.title.match(/::,$/)){
		function df(n){return n>9?n:"0"+n;}
		function GetDATE(){var D = new Date();return D.getFullYear() +"-"+ df(D.getMonth()+1) +"-"+ df(D.getDate());}
		// We modified the pagename, so carefully update ALL other values.
		pg.title=pg.title.replace(/,$/, GetDATE());
		pg.ns=woas.get_namespace(pg.title,true);
		pg.ns=pg.ns.substring(0,pg.ns.length-2);
		pg.cr=pg.title.substring(pg.ns.length+2);
	}
	// check for _Template
	var P = pg.title.split("::");
	pg.name = P.pop();
	while(P){
		var T = P.join("::") + "::" + "_Template";
		if(page_titles.indexOf(T)>-1)
			return woas.get_text(T).replace(/%PAGE%(\w+)%/g, function(t,a){return pg[a]});
		P.pop();
	}

	return "= "+pg.cr+"\n"; // Sigh, nothing found, revert to defaults
}

