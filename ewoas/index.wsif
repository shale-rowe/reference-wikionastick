/* diary2 plugin */

woas.macro.register('woas.diary', function(macro) {
var D = new Date(),
    DAYNAMES = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"],
    MONTHNAMES = ["01", "02", "03", "04", "05", "06", "07", "08", "09", "10", "11", "12"],
    day  = D.getDate();
if(day < 10) day = "0" + day;

var newpagename = "Diary::" + D.getFullYear() +"::"+ MONTHNAMES[D.getMonth()] +"::"+ day +" "+DAYNAMES[D.getDay()];

macro.text = "[["+newpagename+"|Daily diary]]";
macro.reprocess = true;
return true;
});


woas.parser.after_parse = Add2Function(woas.parser.after_parse, "woas.parser.diary(P);", true);
 
woas.parser.diary=function(P){
	// don't go anywhere if editing (seems to never happen)
	if (woas.ui.edit_mode)return;
	if(woas.parser.render_title.match(/^Diary::/)){
		// a helper function
		function cornernote(t,p){ return '<span id="cornernote" style="position:absolute;'+p+':10px;top:3px">'+t+'</span>' }

		var title = woas.parser.render_title;
		var text = "";

		DIARYPAGES=[];
		var i = page_titles.length;
		while(--i>=0){
			if(page_titles[i].match(/^Diary::/)) DIARYPAGES.push(page_titles[i]);
		}
		DIARYPAGES = DIARYPAGES.sort();
		// if the current page is inside DIARYPAGES, it will be bigger than -1
		if((i=DIARYPAGES.indexOf(title))>-1){
			var prev = i>0? i-1 : -1;
			var next = i<DIARYPAGES.length? i+1 : -1;

			text += prev>-1? cornernote( "\u00ab[["+DIARYPAGES[prev]+"]]", "left"):"";
			text += next<DIARYPAGES.length? cornernote("[["+DIARYPAGES[next]+"]]\u00bb", "right"):"";
		}else{
			//text += cornernote("No diary pages for"+title, "left")
		}
		var T={body:text};
		woas.parser.syntax_parse(T, []);
		P.body += T.body;
		//if(document.title.match(/^Editing/))woas.parser.render_title="";
	}
}

