.search_highlight {
	background-color: yellow;
} 

/***** SPOILERS *****/
.spoiler {
	background-color: black;
	color: black;
}
.spoiler:hover {
	background-color: black;
	color: white;
}
.spoilerblock a:link {
	text-decoration: none;
}
.spoilerblock a:visited {
	text-decoration: none;
}
.spoilerblock a:hover .show {
	background-color: black;
	color: white;
	display: block;
}
.spoilerblock a:active {
	text-decoration: none;
}
.spoilerblock a span {
	display: none;
}
.spoilerinline a:link {
	text-decoration: none;
}
.spoilerinline a:visited {
	text-decoration: none;
}
.spoilerinline a:hover .show {
	background-color: black;
	color: white;
	display: inline;
}
.spoilerinline a:active {
	text-decoration: none;
}
.spoilerinline a span {
	display: none;
}

/***** TABLE *****/
table.woas_text_area {
	border: 1pt solid black;
	border-spacing: 0em;
}
table.woas_text_area td {
	border: 1pt solid;
	padding: 0.313em;
}
table.woas_text_area caption {
        font-style: italic;
        font-weight: bold;
}

/***** TABLE - FANCY *****/
table.shadesofblue {
	background-color: #fafafa;
	border-collapse: collapse;
	border-spacing: 0px;
	border: 1px solid #6699CC;
	color: #404040;
	font-family: Verdana, sans-serif, Arial;
	font-size: 14px;
	font-weight: normal;
	/*width: 500px;*/
}
caption.shadesofblue {
	border: 1px solid #6699CC;
	color: #404040;
	text-align: center;
	width: 580px;
}
table.shadesofblue th { 
	background-color: #BEC8D1;
	border-bottom: 2px solid #6699CC;
	border-left: 1px solid #6699CC;
	color: #404040;
	font-weight: bold;
	padding-right: 0.5em;
	padding-left: 0.5em;
	text-indent: 2px;
}
table.shadesofblue td {
	border-top: 0px;
	border-right: 0px;
	border-bottom: 1px solid #9CF;
	border-left: 1px solid #9CF;
	color: #404040;
	padding-right: 0.5em;
	padding-left: 0.5em;
