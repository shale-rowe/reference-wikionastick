<div id="imagesGallery"></div>
<script>
var IMAGES=[];
for(var i=page_titles.length-1;i>=0;--i){
 if(page_titles[i].match(/^%1/))
  IMAGES.push(page_titles[i]);
}
IMAGES = IMAGES.sort();
var iHTML="";
for(var i=IMAGES .length-1;i>=0;--i)
  iHTML += woas.parser.parse("[[Include::"+IMAGES[i]+"]][["+IMAGES[i]+"]]<br>");
$('imagesGallery').innerHTML = iHTML;
</script><!-- This script grabs all images in the specified namespace (represented above by %1) and displays them on one page with clickable links to their respective pages next to each. -->
