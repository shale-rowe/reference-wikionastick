= ToDo list
<div id='todos' style="border:1px solid grey;"></div>
<script>
function listtodo() {
	var x=0;
	var resx = '<table cellspacing=\'2\' cellpadding=\'2\'>';
	var re = /\[\[Include::Template::TODO\|([^\|]+)\|([^\]]+)/g;
	for (var i = 0,l=pages.length; i < l; ++i) {
		var pagetxt = pages[i];
		var res = pagetxt.toString().match(re);
		if (res != null) {
			for (var j = 0,rl=res.length; j<rl ; ++j) {
				var parts = res[j].split('|');
				var anchor = parts[1].replace('_',/ /);
				resx += '<tr><td>' + "<a class=\"link\" onclick=\"go_to('" 
					+ RegExp.escape(page_titles[i]) + "'); window.location.hash='"+anchor +"'\">"
					+ page_titles[i] + "<\/a>" + '</td><td>-</td><td>' + woas.parser.parse(parts[2]) 
					+ '</td></tr>';
			}
		}
	}
	$('todos').innerHTML = resx + '</table>';
}
listtodo();
</script><form><input type='button' onclick='listtodo();' value='Refresh the list'/>&nbsp;&nbsp;&nbsp;<input value="Instructions" onclick="go_to('Help::ToDo list')" type="submit" /></form>

--DFQRCTsKlU
