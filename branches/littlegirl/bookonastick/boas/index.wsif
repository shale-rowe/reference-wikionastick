	color: #404040;
	border: 1px solid #6699CC;
	text-align: center;
	width: 580px;
	}
table.mycustomtable th { 
	color: #404040;
	background-color: #BEC8D1;
	border-bottom: 2px solid #6699CC;
	border-left: 1px solid #6699CC;
	font-weight: bold;
	text-indent: 2px;
	}
table.mycustomtable td {
	color: #404040;
	border-top: 0px;
	border-right: 0px;
	border-bottom: 1px solid #9CF;
	border-left: 1px solid #9CF;
	text-indent: 2px;
	}
table.mycustomtable td.special {
	background-color: #ff00ff;
	}

/* CUSTOM CSS FOR DEFAULT TABLES */
table td.special {
	background-color: #ff00ff;
	}
}}}

