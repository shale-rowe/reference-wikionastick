	margin-left: 1.3em;
	padding-left: 0em;
}

/***** TABLE OF CONTENTS - TITLE *****/
p.woas_toc_title {
	font-weight: bold;
	text-align: center;
}

/***** TAG CONTAINER *****/
/* tag container */
div.taglinks {
/*	background-color: #f9f9f9; */
	border: 1pt solid #aaa;
	clear: both;
	margin-top: 1em;
	padding: 0.313em;
}

/***** TAG LINKS *****/
/* styled boxes around tags */
div.taglinks > a {
	background-color: gainsboro;
	border: 2px solid blue;
	/* border-radius: 7px; */
	color: navy;
	font-size: 90%;
	font-weight: bold;
	line-height: 2.4;
