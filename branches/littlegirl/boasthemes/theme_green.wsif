.woas_help_button {
	font-size: 1em;
	margin-top: 0.75em;
	position: fixed;
	right: 0.625em;
	top: 0.188em;
}
.woas_helpcode {
	font-family: monospace;
	padding: 0.438em;
}
.woas_helptext {
	font-family: lucida;
}

/* HOTKEYS */
#woas_custom_accesskeys {
	display: none;
	visibility: hidden;
}

/* LAST MODIFIED LINE */
.woas_page_mts {
	font-size: small;
	margin-top: 0.75em;
	margin-right: 0.80em;
	margin-left: 15.5%;
	z-index: 0;
}

/* LINK - CURSOR */
a {
	cursor: pointer;
}

/* LINK - EXISTENT */
a.woas_link {
	color: blue;
}
a.woas_link:hover {
	text-decoration: underline;
}

/* LINK - EXTERNAL */
a.woas_world_link {
	color: darkgreen;
	text-decoration: underline;
}

/* LINK - NONEXISTENT */
a.woas_unlink {
	color: red;
}
a.woas_unlink:hover {
	text-decoration: underline;
}

/* MAIN TEXT AREA */
div.woas_text_area {/* any text area div */
	background-color: lightgoldenrodyellow;
	border: 1pt solid darkgreen;
	padding: 0.475em;
}
#woas_wiki_area {/* main text area */
	left: 15.5%;
	margin-top: 1.313em;
	margin-bottom: 0.75em;
	position: relative;
	text-align: justify;
	width: 82.3%;
	z-index: 0;
}
#woas_wiki_area ol li {
	margin-left: -0.5em;
}
#woas_wiki_area ul li {
	margin-left: -0.7em;
}

/* MAIN WIKI PANE WHEN LOCKED */
.woas_text_area.locked, #woas_pwd_mask {
	background-color: #eeeeee;
	background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAAECAAAAACMmsGiAAAALHRFWHRDcmVhdGlvbiBUaW1lAGdpbyAyOSBtYXIgMjAwNyAxMTo1MToxMCArMDEwME%2BTNUAAAAAHdElNRQfXAx0JNByGxueSAAAACXBIWXMAAAsSAAALEgHS3X78AAAABGdBTUEAALGPC%2FxhBQAAABlJREFUeNpj%2FM%2FwmYHpMwMvAxOQwcvECyQBO1oE%2BlG5iisAAAAASUVORK5CYII%3D);
}

/* MENU */
#i_woas_menu_area {
	background-color: lightgoldenrodyellow;
	border: 1pt solid darkgreen;
	font-size: 0.75em;
	left: 1%;
	padding: 0.499em;
	width: 13.2%;
}
#woas_menu_area ol li, #woas_ns_menu_area ol li {
	margin-left: -1.55em;
}
#woas_menu_area ul li, #woas_ns_menu_area ul li {
	margin-left: -1.85em;
}
.woas_menu_button {
	margin-top: 0.438em;
	width: 100%;
}

/* NAVIGATION BAR */
#woas_wiki_header {
	background-color: springgreen;
	border: 1pt solid darkgreen;
	left: 0em;
	opacity: .85;
	padding-right: 0.75em;
	top: 0em;
	width: 99%;
	z-index: 1;
}
#woas_title {
	display: inline;
	font-size: 2em;
	font-weight: bold;
	margin-top: 0em;
	margin-bottom: 0em;
	text-shadow: #888 0.13em 0.13em 0.065em;
}

/* NOWIKI TEXT */
.woas_nowiki {
	background-color: palegreen;
	white-space: pre-wrap; /* CSS 3 */
	padding-right: 0.27em;
	padding-left: 0.27em;
}
.woas_nowiki_multiline {
	border: 0.063em solid grey;
	padding-left: 0.4em;
}

/* PAGE LISTING HEADER */
.woas_listing_options {
	background-color: springgreen;
	border: 0.1em solid blue;
	padding: 0.2em;
}

/* SEARCH */
pre.woas_search_results {
	border: 1pt solid #aaa;
	background-color: #f9f9f9;
	padding: 0.313em;
	margin-top: 1em;
	clear: both;
}
.woas_search_highlight {
	background-color: yellow;
}

/* TABLE OF CONTENTS */
div.woas_toc {
	border: 1pt dashed #aaa;
	background-color: springgreen;
	padding: 0.313em;
	width: 50%;
	font-size: 95%;
}
div.woas_toc ol, li {
	margin-left: 3.3em;
	padding-left: 0em;
}
p.woas_toc_title {
	font-weight: bold;
	text-align: center;
}

/* TABLES */
table.woas_text_area {
	border: 1pt solid black;
	border-spacing: 0em;
}
table.woas_text_area td {
	background-color: palegreen;
	border: 1pt solid;
	padding: 0.313em;
}
table.woas_text_area caption {
	font-style: italic;
	font-weight: bold;
}

/* TAG - CONTAINER */
div.woas_taglinks {
	background-color: palegreen;
	border: 1pt solid seagreen;
	margin-top: 1em;
	overflow: auto;
	padding: 0.313em;
}

/* TAG - LINKS */
div.woas_taglinks > a {
	background-color: springgreen;
	border: 2px solid darkgreen;
	/* border-radius: 7px; */
	color: navy;
	font-size: 90%;
	font-weight: bold;
	line-height: 2.4;
	margin: 2px 2px 2px 0;
	-moz-border-radius: 7px;
	padding: 3px 5px 3px 5px;
	text-decoration: none;
	-webkit-border-radius: 7px;
}
div.woas_taglinks > a:hover {
	background-color: grey;
	color: white;
	border: 2px solid blue;
	text-decoration: none;
	font-weight: bold;
}

/* WIKI CORE PAGES */
.woas_core_page {
	background-color: palegreen;
}

/* WIKI LOADING OVERLAY */
#loading_overlay {
	background-color: white;
	font-size: large;
	height: 100%;
	left: 0em;
	padding-top: 25%;
	position: absolute;
	text-align: center;
	top: 0em;
	width: 99%;
	z-index: 100;
}

/* WIKI WAIT TEXT */
.woas_wait_text {
	color: navy;
}

