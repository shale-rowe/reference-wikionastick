/* Your CSS customization goes here */
/* BODY */
body {
	font-family: Georgia, verdana;
}
.woas_background {
	background-color: seagreen;
}

/* BUTTONS */
.woas_button {
	background-color: palegreen;
	border: 1pt solid seagreen;
	color: darkgreen;
	cursor: pointer;
	font-variant: normal;
	margin-top: 0.438em;
	text-align: center;
}
.woas_button:hover {
	background-color: darkgrey;
}

/* DEBUG CONSOLE */
#woas_debug_button {
	margin-top: 0.5%;
}
#woas_debug_console_title {
	color: darkgreen;
	font-weight: bold;
	text-align: left;
}
#woas_debug_log {
	background-color: #9AFF9A;
	border: 1px solid black;
	margin-top: 0.5%;
	width: 100%;
}
#woas_debug_log_clear_button {
	margin-top: 0.3%;
	margin-bottom: 0.3%;
}
#woas_debug_panel {
	background-color: #048db4;
	border: 0;
	margin-top: -0.4em;
}

/* EDITOR */
#woas_edit_page_title{
	margin-top: 2.188em;
}
.woas_editor_button {
	margin-top: 0.063em;
	margin-bottom: 0.063em;
}

/* EMBEDDED FILES */
.woas_embedded {
	background-color: #9E9EFF;
}

/* ENCRYPTION */
.woas_password_desc {
	background-color: #FAFAFB;
	border: 2pt solid navy;
	font-size: 0.8em;
	margin-right: 25%;
	margin-left: 25%;
}
#woas_pwd_mask {
	font-size: large;
	height: 100%;
	left: 0em;
	position: absolute;
	text-align: center;
	top: 0em;
	visibility: hidden;
	width: 99%;
	z-index: 99;
}
#woas_pwd_query {/* ask password overlay */
	background-color: #d6dff7;
	border: 1pt solid #666666; 
	font-size: large;
	height: 100%;
	left: 0em;
	padding-top: 25%;
	position: absolute;
	text-align: center;
	top: 0em;
	visibility: hidden;
	width: 99%;
	z-index: 100;
}

/* FOOTER */
#woas_footer {
	background-color: springgreen;
	border: 1pt solid black;
	color: darkgreen;
	font-size: 95%;
	font-weight: bold;
	line-height: 1em;
	margin-top: 0.75em;
	margin-right: 0.85em;
	margin-left: 15.5%;
	padding-top: 0;
	padding-right: 0.5%;
	padding-bottom: 0;
	padding-left: 0.5%;
}
#woas_footer a {
	color: darkgreen ! important;
}
#woas_go_to_top_button {
	margin-bottom: -0.4em;
}

/* HEADERS */
h1.woas_header {
	font-size: 1.438em;
}
h2.woas_header {
	font-size: 1.25em;
}
h3.woas_header {
	font-size: 1.063em;
}
h4.woas_header {
	font-size: 0.875em;
}
h5.woas_header {
	font-size: 0.75em;
}
h6.woas_header {
	font-size: 0.625em;
}

