/* Your CSS customization goes here */

/*EXAMPLES FAIL TEXT*/
.fail {
	background-color: red;
	color: white;
	font-weight: bold;
	}

/*EXAMPLES INTRODUCTORY TEXT*/
.intro {
	background-color: #048db4;
	color: white;
	font-weight: bold;
	padding: 0.2em;
	}

/*EXAMPLES WRAP-TEST*/
.wrap_test {
/*	background-color: #048db4;*/
	background-color: black;
	color: white;
	font-weight: bold;
	padding-right: 0.2em;
	padding-left: 0.2em;
	}

/* NOWIKI TEXT */
.woas_nowiki {
	background-color: gainsboro;
	}
.woas_nowiki_multiline {
	border: 0.063em solid grey;
	} 

/*SUB-MENU*/
#i_woas_menu_area {
	height: 80%;
	overflow: auto;
	}
#woas_ns_menu_edit_button {
	padding-bottom: 0.4em;
	}

/* TEST QUESTIONS */
.test {
	background-color: powderblue;
	}
.test:before {
	background-color: #048db4;
	color: white;
	content: "TAKE THE TEST";
	display: block;
	font-weight: bold;
	text-align: center;
	} 
.test:after {
	background-color: #048db4;
	color: white;
	content: "\00A0";
	display: block;
	font-weight: bold;
	text-align: center;
	} 

/*TOPNOTES*/
div.topnote {
	position: absolute;
	top: 0.188em;
	right: 0.625em;
	}

