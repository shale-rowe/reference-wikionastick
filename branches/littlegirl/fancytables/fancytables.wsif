{|
|$ style="background:springgreen"
| *Header 1* || *Header 2* || *Header 3*
| ==cell 1 || cell 2 || ==cell 3
| cell 1 || ==cell 2 || cell 3
|}
</td><td align="center">
{| class="shadesofblue"
|$ style="background:springgreen"
| =Header 1 || =Header 2 || =Header 3
| ==cell 1 || cell 2 || ==cell 3
| cell 1 || ==cell 2 || cell 3
|}
</td></tr><tr><td>
