# Open the wiki in a text editor.<br><br>
# *Find* this section:<pre>function &#095;filter&#095;wiki(s) &#123;<br>	return s.replace(&#047;&#092;&#123;&#092;&#123;&#092;&#123;((.|&#092;n)&#042;?)&#092;&#125;&#092;&#125;&#092;&#125;&#047;g, "").<br>		replace(&#047;&lt;script&#091;^&gt;&#093;&#042;&gt;((.|&#092;n)&#042;?)&lt;&#092;&#047;script&gt;&#047;gi, "").<br>		replace(&#047;&#092;&lt;&#092;w+&#092;s&#091;^&gt;&#093;+&gt;&#047;g, "").<br>		replace(&#047;&#092;&lt;&#092;&#047;&#092;w&#091;^&gt;&#093;+&gt;&#047;g, "");<br>&#125;/pre>
# *Replace* it with this section:<pre>function &#095;filter&#095;wiki(s) &#123;<br>	var nowiki = s.match(&#047;&#092;&#123;&#092;&#123;&#092;&#123;((.|&#092;n)&#042;?)&#092;&#125;&#092;&#125;&#092;&#125;&#047;g);<br>	var t = s.replace(&#047;&#092;&#123;&#092;&#123;&#092;&#123;((.|&#092;n)&#042;?)&#092;&#125;&#092;&#125;&#092;&#125;&#047;g, "").<br>		replace(&#047;&lt;script&#091;^&gt;&#093;&#042;&gt;((.|&#092;n)&#042;?)&lt;&#092;&#047;script&gt;&#047;gi, "").<br>		replace(&#047;&#092;&lt;&#092;w+&#092;s&#091;^&gt;&#093;+&gt;&#047;g, "").<br>		replace(&#047;&#092;&lt;&#092;&#047;&#092;w&#091;^&gt;&#093;+&gt;&#047;g, "");<br>	if (nowiki) &#123;<br>		for (var i = 0; i &lt; nowiki.length; ++i)<br>			t += (nowiki&#091;i&#093;.replace (&#047;&#092;&#123;&#092;&#123;&#092;&#123;&#047;g, " ").replace (&#047;&#092;&#125;&#092;&#125;&#092;&#125;&#047;g, " "));<br>	&#125;<br>	return t;<br>&#125;</pre>
