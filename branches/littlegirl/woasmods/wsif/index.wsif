= Fix image embedding
This makes it so image embedding works properly when using wiki image links ({{{[[Image::NAME]]}}} and {{{[[Include::Image::NAME]]}}}).
# Open the wiki in a text editor.<br><br>
# *Find* this function:<pre>woas&#091;"&#095;embed&#095;process"&#093; = function(etype) &#123;<br>	var filename = &#036;("filename&#095;").value;<br>	if(filename == "") &#123;<br>		alert("A file must be selected");<br>		return false;<br>	&#125;<br><br>	var ct = mozillaLoadFileID(field&#095;id);<br>	if(ct)&#123;<br>		if (etype == "image")<br>			etype = 12;<br>		else<br>			etype = 4;<br>	&#125;else&#123;<br>		&#095;force&#095;binary = true;<br>		ct = loadFile(filename, field&#095;id);<br>		&#095;force&#095;binary = false;<br>		if (ct == null || !ct.length) &#123;<br>			alert("Could not load file "+filename);<br>			return false;<br>		&#125;<br><br>		ct = encode64(ct);<br><br>		&#047;&#047; calculate the flags for the embedded file<br>		if (etype == "image") &#123;<br>			var m=filename.match(&#047;&#092;.(&#092;w+)&#036;&#047;);<br>			if (m==null) m = "";<br>			else m=m&#091;1&#093;.toLowerCase();<br>			var guess&#095;mime = "image";<br>			switch (m) &#123;<br>				case "png":<br>					guess&#095;mime = "image&#047;png";<br>				break;<br>				case "gif":<br>					guess&#095;mime = "image&#047;gif";<br>					break;<br>				case "jpg":<br>				case "jpeg":<br>					guess&#095;mime = "image&#047;jpeg";<br>					break;<br>			&#125;<br>			ct = "data:"+guess&#095;mime+";base64,"+ct;<br>			etype = 12;<br>		&#125; else etype = 4;<br>	&#125;<br><br>	pages.push(ct);<br>	page&#095;attrs.push(etype);<br>	page&#095;titles.push(current);<br><br>	&#047;&#047; save everything<br>	this.save&#095;to&#095;file(true);<br><br>	this.refresh&#095;menu&#095;area();<br>	this.set&#095;current(current, true);<br><br>	return true;<br>&#125;</pre>
# *Replace* it with this function:<pre>woas&#091;"&#095;embed&#095;process"&#093; = function(etype) &#123;<br>	var field&#095;id = "filename&#095;";<br>	var filename = &#036;(field&#095;id).value;<br>	if(filename == "") &#123;<br>		alert("A file must be selected");<br>		return false;<br>	&#125;<br><br>	var ct = mozillaLoadFileID(field&#095;id);<br>	if(ct)&#123;<br>		if (etype == "image")<br>			etype = 12;<br>		else<br>			etype = 4;<br>	&#125;else&#123;<br>		&#095;force&#095;binary = true;<br>		ct = loadFile(filename, field&#095;id);<br>		&#095;force&#095;binary = false;<br>		if (ct == null || !ct.length) &#123;<br>			alert("Could not load file "+filename);<br>			return false;<br>		&#125;<br><br>		ct = encode64(ct);<br><br>		&#047;&#047; calculate the flags for the embedded file<br>		if (etype == "image") &#123;<br>			var m=filename.match(&#047;&#092;.(&#092;w+)&#036;&#047;);<br>			if (m==null) m = "";<br>			else m=m&#091;1&#093;.toLowerCase();<br>			var guess&#095;mime = "image";<br>			switch (m) &#123;<br>				case "png":<br>					guess&#095;mime = "image&#047;png";<br>				break;<br>				case "gif":<br>					guess&#095;mime = "image&#047;gif";<br>					break;<br>				case "jpg":<br>				case "jpeg":<br>					guess&#095;mime = "image&#047;jpeg";<br>					break;<br>			&#125;<br>			ct = "data:"+guess&#095;mime+";base64,"+ct;<br>			etype = 12;<br>		&#125; else etype = 4;<br>	&#125;<br>	pages.push(ct);<br>	page&#095;attrs.push(etype);<br>	page&#095;titles.push(current);<br><br>	&#047;&#047; save everything<br>	this.save&#095;to&#095;file(true);<br><br>	this.refresh&#095;menu&#095;area();<br>	this.set&#095;current(current, true);<br><br>	return true;<br>&#125;</pre>
# *Find* this function:<pre>function saveFile(fileUrl, content)<br>&#123;<br>	var r = null;<br>	r = mozillaSaveFile(fileUrl, content);<br>	if((r == null) || (r == false))<br>		r = ieSaveFile(fileUrl, content);<br>	if((r == null) || (r == false))<br>		r = javaSaveFile(fileUrl, content);<br>	return r;<br>&#125;<br></pre>
# *Add* this function *beneath* it:<pre>function mozillaLoadFileID(field&#095;id)&#123;<br>	var filename = document.getElementById(field&#095;id).value;<br>	if(filename == "")<br>		return false;<br>	if(window.Components &amp;&amp;amp; document.getElementById(field&#095;id).files)<br>		return document.getElementById(field&#095;id).files.item(0).getAsDataURL(); &#047;&#047; .getAsBinary() .getAsText()<br>	return null;<br>&#125;</pre>
