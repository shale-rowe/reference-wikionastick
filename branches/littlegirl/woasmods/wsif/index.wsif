<script>
function menu_key_hook(orig_e) {
    var e;
    if (!orig_e)
        e = window.event;
    else
        e = orig_e;
	
    if (e.keyCode==13) {
	ff_fix_focus();
	_raw_do_search($('menu_string_to_search').value);
        return false;
     }
     return orig_e;
}

$('menu_string_to_search').onkeydown = menu_key_hook;
</script>

